{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es6",
   
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react"
  },

  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
